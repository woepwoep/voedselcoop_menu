<?xml version="1.0" encoding="UTF-8"?>
<openerp><data>

<record id="vergelijkingsscherm_tree_view" model="ir.ui.view">
    <field name="name">vergelijkingsscherm.tree</field>
    <field name="model">product.template</field>
    <field name="arch" type="xml">
        <tree string="Vergelijkingsscherm">
			<field name="name"/>
			<field name="merk"/>
			<field name="kwaliteit"/>
			<field name="uom_id" invisible="0"/>
			<field name="inhoud"/>
			<field name="eenheid"/>
			<field name="standard_price"/>
			<field name="list_price"/>
			<field name="pos_categ_id"/>
			<field name="categ_id"/>
			<field name="available_in_pos"/>
			<field name="active"/>
        </tree>
    </field>
</record>

<record id="vergelijkingsscherm_form_view" model="ir.ui.view">
    <field name="name">vergelijkingsscherm.form</field>
    <field name="model">product.template</field>
    <field name="arch" type="xml">
        <form string="Vergelijkingsscherm view">
            <sheet>		
                <div class="oe_left" style="width: 500px;">
					<field name="image_medium" widget="image" class="oe_avatar oe_left" modifiers="{}"/>
					<div class="oe_title" style="width: 780px;">
						<label class="oe_edit_only" for="name" string="Product Name"/>
						<h1>
							<field name="name" class="oe_inline" modifiers="{'required': true}"/>
						</h1>
					</div>
				</div>
                <notebook>
                	<page name="extended_information" string="Artikelbeheer">
						<group name="group_top">
							<group name="group_left">
								<field name="available_in_pos"/>
								<field name="ean13"/>
								<field name="product_variant_count" invisible="1"/>
								<button colspan="2" name="%(point_of_sale.action_edit_ean)d" type="action" string="Set a Custom EAN"
                            		attrs="{'invisible': [('product_variant_count', '>', 1)]}" class="oe_link oe_edit_only"/>
								
								<field name="categ_id"/>
								<field name="pos_categ_id"/>
								<field name="merk"/>
								<field name="inhoud"/>
								<field name="eenheid"/>
								<field name="uom_id"/>
						        <field name="use_deposit"/>
						        <field name="select_deposit" attrs="{'invisible': [('use_deposit', '=', False)]}"/>
							</group>
							<group name="group_right">
								<field name="active"/>
								<field name="weegschaalartikel"/>
								<field name="to_weight"/>
								<field name="pluartikel"/>
								<field name="plucode"/>
								<field name="taxes_id" colspan="2" widget="many2many_tags"/>
								<field name="list_price"/>
								<field name="standard_price"/>
								<field name="kwaliteit"/>
								<field name="herkomst"/>
							</group>
						</group>
						<separator string="Suppliers"/>
						<field name="seller_ids"/>
						<separator string="IngrediÃ«nten"/>
						<group name="ingredienten">
							<field name="ingredienten"/>
						</group>
						<group name="group_top">
							<group name="group_left">
								<field name="proefdiervrij"/>
								<field name="vegetarisch"/>
								<field name="d204"/>
								<field name="d209"/>
								<field name="d210"/>
								<field name="d212"/>
								<field name="d213"/>
								<field name="d214"/>
								<field name="d234"/>
								<field name="d215"/>
								<field name="d239"/>
								<field name="d216"/>
								<field name="d217"/>
								<field name="d217b"/>
								<field name="d220"/>
								<field name="d221"/>
								<field name="d221b"/>
							</group>
							<group name="group_right">
								<field name="veganistisch"/>
								<field name="rauwemelk"/>
								<field name="d222"/>
								<field name="d223"/>
								<field name="d236"/>
								<field name="d238"/>
								<field name="d238b"/>
								<field name="d225"/>
								<field name="d226"/>
								<field name="d228"/>
								<field name="d230"/>
								<field name="d232"/>
								<field name="d237"/>
								<field name="d240"/>
								<field name="d241"/>
								<field name="d242"/>
							</group>
						</group>
						<separator string="Overige gegevens"/>
						<group name="group_top">
							<group name="group_left">
								<field name="aantaldagenhoudbaar"/>
								<field name="bewaartemperatuur"/>
								<field name="gebruikstips"/>
							</group>
						<group name="group_right">
						</group>
						</group>						
					</page>
                </notebook>
            </sheet>
        </form>
    </field>
</record>


<record model="ir.actions.act_window.view" id="vergelijkingsscherm_tree_view_action">
    <field name="view_id" ref="vergelijkingsscherm_tree_view" />
    <field name="act_window_id" ref="view_action_vergelijkingsscherm" />
    <field name="sequence" eval="19" />
    <field name="view_mode">tree</field>
</record>

<record model="ir.actions.act_window.view" id="vergelijkingsscherm_form_view_action">
    <field name="view_id" ref="vergelijkingsscherm_form_view" />
    <field name="act_window_id" ref="action_view_vergelijkingsscherm" />
    <field name="sequence" eval="20" />
    <field name="view_mode">form</field>
</record>

</data></openerp>
